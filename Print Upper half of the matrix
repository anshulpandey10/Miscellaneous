#include<iostream>
#include<conio.h>
main()
{
	int a[3][3],i,j;
	printf("enter the value of the matrix \n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	
	
	
	
printf("the resulted matrix is \n");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{	
if(i>=j)
{
printf("%d",a[i][j]);
printf(" ");
}
}
printf("\n");
}

}


