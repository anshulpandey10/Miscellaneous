#include<iostream>
using namespace std;
int count=0;
int main()
{
   int a[5]={1,2,3,2,1};
    int n=5;
    
    for(int i=0;i<(n/2)+1;i++)
    {
    	count=0;
      for(int j=(n/2)+1;j<n;j++)  
      {
        
          if(a[i]==a[j])
          {  
		  count=count+1;   }
        }
      
       if(count==0)
       {
       	cout<<a[i]; 
	   }
        
            
    }


	
}
