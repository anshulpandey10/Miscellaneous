#include<iostream>
#include<string.h>
using namespace std;
int main()
{
	char s1[4]={'A','B','C','D'};
    char s2[4]={'C','D','A','B'};
    
    char s3[8]={'A','B','C','D','A','B','C','D'};
    
	
	for(int i=0; i<8;i++)
	{
		if(s2[0]==s3[i] && s2[1]==s3[i+1] && s2[2]==s3[i+2] && s2[3] == s3[i+3])
		{
			cout<<"found";
			}
			}
		}
		
